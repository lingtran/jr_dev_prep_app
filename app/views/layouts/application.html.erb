<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">

    <%= stylesheet_link_tag 'application' %>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tags %>
    <title>Jr Dev Interview Prep</title>
  </head>
  <body>

    <%= render '/layouts/navbar' %>

    <div class="menu_flash">
      <% flash.each do |name, msg| %>
        <%= content_tag :div, msg, :id => "flash_{name}" %>
      <% end %>
    </div>

    <div class="container">
      <%= yield %>
    </div>

  </body>
</html>
